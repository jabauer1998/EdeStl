modules = ["java-graalvm22.3", "python-3.11"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["openjfx", "xorg.libX11", "xorg.libXext", "xorg.libXrender", "xorg.libXtst", "xorg.libXi"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Build"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Run Sample"

[[workflows.workflow]]
name = "Build"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "bash build/LinuxBuild.sh build"

[[workflows.workflow]]
name = "Run Sample"
author = "agent"

[workflows.workflow.metadata]
outputType = "vnc"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd /home/runner/workspace && tools/jdk-25.0.2/bin/java -cp \"bin/EdeStl.jar:bin/EdeSample.jar:lib/Declan.jar\" sample.ede.Processor"
